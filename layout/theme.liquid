

<!doctype html>
<!--[if IE 8]><html class="no-js ie8" lang="en"> <![endif]-->
<!--[if IE 9]><html class="no-js ie9" lang="en"> <![endif]-->
<!--[if (gt IE 10)|!(IE)]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
  <!-- Canopy v1.0.1 -->
  <meta charset="utf-8" />
  <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1' /><![endif]-->

  <link rel="shortcut icon" type="image/x-icon" href="{{ 'favicon.ico' | asset_url }}">

  <title>
    {{ page_title }}{% if current_tags %} &ndash; {{ 'general.meta.tagged' | t }} "{{ current_tags | join: ', ' }}"{% endif %}
    {% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t }} {{ current_page }}{% endif %}
    {% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
    {% if template == "customers/login" %} &ndash; Login{% endif %}
    {%if template == "customers/activate_account"%} &ndash; Activate Account{% endif %}
  </title>

  {% if page_description %}
  <meta name="description" content="{{ page_description | escape }}" />
  {% endif %}

  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <link rel="canonical" href="{{ canonical_url }}" />
  {{ '//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js' | script_tag }}

  <!-- Compiled and minified CSS -->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  {{ 'materialize.css' | asset_url | stylesheet_tag }}

  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.5/handlebars.min.js"></script>
  <script src="https://cdn.ravenjs.com/3.2.1/raven.min.js"></script>
  <script src="https://www.gstatic.com/firebasejs/live/3.0/firebase.js"></script>

  {% include 'fb-open-graph-tags' %}
  {% include 'twitter-card' %}


  {% comment %} Google Fonts {% endcomment %}
  {% assign gfonts = '' %}
  {% assign setting_indexes = 'body_font|heading_font|nav_font|logo_font' | split: '|'%}
  {% for index in setting_indexes %}
  {% if settings[index] contains 'GWF' %}
  {% assign custom_index = index | append: '_custom' %}
  {% capture gfonts %}{{ gfonts }}|{% if settings[index] contains 'GWF|CUSTOM' %}{{ settings[custom_index] }}{% else %}{{ settings[index] | split: '|' | last }}{% endif %}{% endcapture %}
  {% endif %}
  {% endfor %}

  {% if gfonts != '' %}
    <link href="//fonts.googleapis.com/css?family={{ gfonts | remove_first: '|' | replace: ' ', '+' | replace: "'", '' }}" rel="stylesheet" type="text/css">

    {{ 'styles.scss.css' | asset_url | stylesheet_tag }}

    <!--[if lt IE 9]>
      {{ '//html5shiv.googlecode.com/svn/trunk/html5.js' | script_tag }}
      {{ 'selectivizr-min.js' | asset_url | script_tag }}
      {{ 'ie8.css' | asset_url | stylesheet_tag }}
    <![endif]-->



    {{ 'option_selection.js' | shopify_asset_url | script_tag }}
    {{ 'api.jquery.js' | shopify_asset_url | script_tag }}

{% endif %}




    {{ content_for_header }}


    <script>
      document.documentElement.className = document.documentElement.className.replace('no-js', '');
      products = {};
      Shopify.money_format = {{ shop.money_format | json }};
    </script>

    <!-- Dorrbell Adboe TypeKit -->
    <script src="https://use.typekit.net/jey4hge.js"></script>
    <script>try{Typekit.load({ async: true });}catch(e){}</script>

    {{ 'dorrbell-styles.scss.css' | asset_url | stylesheet_tag }}
    {% include 'bacon-menu' %}
  </head>

  <body class="template-{{ template | split: '.' | first }}" {% if template == "customers/login" %}style="margin:0;"{%endif%}{%comment%}{% if template == "customers/login" %}style="background-image:url({{ 'background.gif' | asset_url }});"{% endif %}{%endcomment%}>
    {% include 'firebase' %}
    {% include 'db' %}

    <div class="navbar-fixed">
      <nav>
        <div class="nav-wrapper">
          <a href="#" class="brand-logo valign-wrapper">
            {{ 'logo.png' | asset_url | img_tag }}
          </a>
          <ul id="slide-out" class="side-nav">
            <li><a href="#!">First Sidebar Link</a></li>
            <li><a href="#!">Second Sidebar Link</a></li>
            <li class="no-padding">
              <ul class="collapsible collapsible-accordion">
                <li>
                  <a class="collapsible-header">Dropdown<i class="mdi-navigation-arrow-drop-down"></i></a>
                  <div class="collapsible-body">
                    <ul>
                      <li><a href="#!">First</a></li>
                      <li><a href="#!">Second</a></li>
                      <li><a href="#!">Third</a></li>
                      <li><a href="#!">Fourth</a></li>
                    </ul>
                  </div>
                </li>
              </ul>
            </li>
          </ul>

          <ul class="right hide-on-med-and-down">
            <li class="input-field">
                <form action="/search" method="get">
                  {% if settings.search_prods_only %}<input type="hidden" name="type" value="product">{% endif %}
                  <input id="search" type="search" required name="q" placeholder="{{ 'layout.header.search' | t }}"/>
                </form>
            </li>
            <li>
                <a href="sass.html"><i class="material-icons">search</i></a>
            </li>
            <li><a href="/pages/referrals" id="referral-link">$0 Credit</a></li>
            <li>
              <a class="waves-effect waves-light btn">
                <i class="material-icons left">shopping_cart</i>
                Cart
                <span class="badge new">4</span>
              </a>
            </li>
          </ul>
          <a href="#" data-activates="slide-out" class="button-collapse"><i class="material-icons">menu</i></a>
        </div>
      </nav>
    </div>
    <div>
      {{content_for_layout}}
    </div>

    {% include 'mobile-navigation' %}

    {% include 'cart-summary' %}

    {{ 'libs.js' | asset_url | script_tag }}
    {{ 'scripts.js' | asset_url | script_tag }}

    <input type="hidden" name="_pc_params" value="{{ shop.permanent_domain }}:{{ customer.id }}" />

    <script>
      // Initialize collapse button
      $(".button-collapse").sideNav();
      $('.collapsible').collapsible();
    </script>

  </body>
  </html>
